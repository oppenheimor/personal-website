{
  "rewrites": [
    {
      "source": "/ai-prompt-card/:path*",
      "destination": "https://random-prompt-card-3sst.vercel.app/:path*"
    },
    {
      "source": "/ai-prompt-card",
      "destination": "https://random-prompt-card-3sst.vercel.app/"
    },
    {
      "source": "/:path*",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/ai-prompt-card/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "SAMEORIGIN"
        }
      ]
    }
  ],
  "trailingSlash": false
}