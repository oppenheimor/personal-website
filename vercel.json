{
  "redirects": [
    {
      "source": "/ai-prompt-card",
      "destination": "https://random-prompt-card-3sst.vercel.app/",
      "permanent": false
    },
    {
      "source": "/ai-prompt-card/(.*)",
      "destination": "https://random-prompt-card-3sst.vercel.app/$1",
      "permanent": false
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    }
  ],
  "trailingSlash": false
}